{
  "name": "react-particles-text",
  "version": "0.0.4",
  "author": "Ahmad Sabry",
  "description": "A React component for creating interactive particle text effects",
  "homepage": "https://react-particles-text.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/A7madSab/react-particles-text.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "particles",
    "text",
    "animation",
    "interactive",
    "canvas"
  ],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "test": "jest --watchAll --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "not dead",
              "not ie <= 11"
            ]
          }
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {}
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.23",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
